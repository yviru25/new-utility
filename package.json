{
  "name": "compliance-utility",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "ng build && electron .",
    "electron-packager": "electron-packager . Compliance-Utility --ignore=node_modules",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --ignore=node_modules/electron --prune=true --out=release-builds",
    "package-win": "electron-packager . compliance-utility --platform win32",
    "electron-build": "electron-packager . --all"
  },
  "build": {
    "appId": "com.compliance",
    "mac": {
      "category": "Utility"
    }
  },
  "private": true,
